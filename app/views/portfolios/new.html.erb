<h1>Portfolios#new</h1>
<%= form_for @portfolio do |f| %>
<div class="">
  <%= f.label :title %>
  <%= f.text_field :title %>
</div>
<div class="">
  <%= f.label :subtitle %>
  <%= f.text_field :subtitle %>
</div>

<div class="">
  <%= f.label :body %>
  <%= f.text_area :body %>
</div>

<div class="">
  <%= f.submit %>
</div>
<% end %>
